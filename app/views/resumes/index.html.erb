<h1><%= "#{current_user.name}'s Resumes" %></h1>
<div id = "create_resume_form">
	<%=form_for(Resume.new, remote: true) do |f|%>
		<%=f.label :name%>
		<%=f.text_field :name%> </br>
		<%=f.label :description%>
		<%=f.text_field :description%></br>
		<%=f.submit "Create New Resume"%>
	<%end%>
<div>

<table id="resume_list">
		<thead>
			<tr>
				<th> Resume Name </th>
				<th> Description </th>
			</tr>
		</thead>	
		<tbody>
			<% @resumes.each do |resume|%>
				<%= render resume%>
			<%end%>
		</tbody>
</table>
<%#=create_resume_list(@resumes)%>









